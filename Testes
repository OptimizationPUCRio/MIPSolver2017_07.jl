using JuMP
using Cbc

#Custo UnitÃ¡rio
c = [2; 3; 2; 5]


#Custo Fixo
f = [1; 3; 1; 5]

m= Model(solver = CbcSolver())

@variable(m, x[i=1:4]>=0)

@variable(m,y[i=1:4], Bin)

@constraint(m, sum(x[j] for j=1:4) >=10)

@constraint(m,x[1]<=5*y[1])

@constraint(m,x[2]<=4*y[2])

@constraint(m,x[3]<=3*y[3])

@constraint(m,x[4]<=2*y[4])

@objective(m, Min, sum(f[j]*y[j]+c[j]*x[j] for j=1:4))
